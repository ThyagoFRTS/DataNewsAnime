<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.datanewsanime">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_sakura"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_sakura_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".activities.SearchResults" />
        <activity android:name=".activities.MainActivityIntents" />
        <activity android:name=".activities.Profile" />
        <activity android:name=".activities.Home" />
        <activity android:name=".activities.NewsContent" />
        <activity android:name=".activities.UserRegister" />
        <activity android:name=".activities.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>